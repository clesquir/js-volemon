  "description": "HTML5 version of the Volemon (or Slime Volley) game implemented in Meteor",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "async": "^2.6.1",
    "buffer": "^5.2.1",
    "bufferutil": "^3.0.5",
    "chai": "^4.2.0",
    "chart.js": "^2.7.3",
    "component-emitter": "^1.2.1",
    "core-js": "^2.5.7",
    "detectrtc": "^1.3.6",
    "express": "^4.16.4",
    "has-binary": "^0.1.7",
    "hat": "^0.0.3",
    "he": "^1.2.0",
    "hotkeys-js": "^3.3.8",
    "jquery-lazy": "^1.7.10",
    "keycode": "^2.2.0",
    "lodash": "^4.17.11",
    "lodash.throttle": "^4.1.1",
    "meteor-node-stubs": "^0.4.1",
    "phaser-ce": "^2.11.1",
    "rollbar": "^2.4.7",
    "sc-cluster-broker-client": "^1.0.5",
    "simple-peer": "^9.1.2",
    "sinon": "^7.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "socket.io-parser": "^3.2.0",
    "socketcluster": "^14.3.2",
    "socketcluster-client": "^14.2.1",
    "swiper": "^4.4.1",
    "synaptic": "^1.1.4",
    "utf-8-validate": "^4.0.2",
    "write": "^1.0.3"
  },
  "devDependencies": {
    "nightmare": "^3.0.1"
  },
  "scripts": {
    "meteor-dev": "meteor --settings=settings-dev.json",
    "meteor-rollbar": "meteor --settings=settings-rollbar.json",
    "test": "TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-web": "TEST_BROWSER_DRIVER=nightmare meteor test --driver-package=practicalmeteor:mocha --port 4000",
    "sshkey": "eval \"$(ssh-agent -s)\" && ssh-add ~/.ssh/id_rsa",
    "reconfig": "mup reconfig --settings=settings-prod.json",
    "setup": "mup setup --settings=settings-prod.json",
    "proxy-reconfig": "mup proxy reconfig-shared --settings=settings-prod.json",
    "deploy": "mup deploy --settings=settings-prod.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clesquir/js-volemon.git"
  },
  "keywords": [
    "volemon",
    "slimevolley",
    "multiplayer"
  ],
  "author": "Cedric Lesquir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clesquir/js-volemon/issues"
