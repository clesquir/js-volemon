{
  "name": "js-volemon",
  "version": "1.0.0",
  "description": "HTML5 version of the Volemon (or Slime Volley) game implemented in Meteor",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "@types/jquery": "^3.3.29",
    "@types/meteor": "^1.4.27",
    "async": "^2.6.2",
    "buffer": "^5.2.1",
    "bufferutil": "^3.0.5",
    "chai": "^4.2.0",
    "chart.js": "^2.8.0",
    "component-emitter": "^1.2.1",
    "core-js": "^2.6.5",
    "detectrtc": "^1.3.9",
    "express": "^4.16.4",
    "has-binary": "^0.1.7",
    "hat": "^0.0.3",
    "he": "^1.2.0",
    "hotkeys-js": "^3.6.2",
    "jquery-lazy": "^1.7.10",
    "keycode": "^2.2.0",
    "lodash": "^4.17.11",
    "lodash.throttle": "^4.1.1",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "phaser-ce": "^2.12.0",
    "rollbar": "^2.5.4",
    "sc-cluster-broker-client": "^1.0.5",
    "simple-peer": "^9.3.0",
    "sinon": "^7.3.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-parser": "^3.3.0",
    "socketcluster": "^14.3.3",
    "socketcluster-client": "^14.2.2",
    "swiper": "^4.5.0",
    "synaptic": "^1.1.4",
    "utf-8-validate": "^4.0.2",
    "write": "^1.0.3"
  },
  "devDependencies": {
    "chromedriver": "^2.43.1",
    "nightmare": "^3.0.1",
    "phantomjs-prebuilt": "^2.1.16",
    "selenium-webdriver": "3.6.0"
  },
  "scripts": {
    "meteor-dev": "meteor --settings=settings-dev.json",
    "meteor-rollbar": "meteor --settings=settings-rollbar.json",
    "test": "TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-phantom": "TEST_BROWSER_DRIVER=phantomjs meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-nightmare": "TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-chrome": "TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-web": "meteor test --driver-package=practicalmeteor:mocha --port 4000",
    "sshkey": "eval \"$(ssh-agent -s)\" && ssh-add ~/.ssh/id_rsa",
    "reconfig": "mup reconfig --settings=settings-prod.json",
    "setup": "mup setup --settings=settings-prod.json",
    "proxy-reconfig": "mup proxy reconfig-shared --settings=settings-prod.json",
    "deploy": "mup deploy --settings=settings-prod.json",
    "get-phaser-typings": "curl -o definition-typings/phaser.d.ts https://raw.githubusercontent.com/photonstorm/phaser-ce/master/typescript/phaser.d.ts && curl -o definition-typings/pixi.d.ts https://raw.githubusercontent.com/photonstorm/phaser-ce/master/typescript/pixi.d.ts && curl -o definition-typings/p2.d.ts https://raw.githubusercontent.com/photonstorm/phaser-ce/master/typescript/p2.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clesquir/js-volemon.git"
  },
  "keywords": [
    "volemon",
    "slimevolley",
    "multiplayer"
  ],
  "author": "Cedric Lesquir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clesquir/js-volemon/issues"
  },
  "homepage": "https://github.com/clesquir/js-volemon#readme"
}
