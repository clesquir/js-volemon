  "scripts": {
    "meteor-dev": "meteor --settings=settings-dev.json",
    "meteor-rollbar": "meteor --settings=settings-rollbar.json",
    "test": "TEST_BROWSER_DRIVER=phantomjs meteor test --once --driver-package meteortesting:mocha --port 4000",
    "test-web": "meteor test --driver-package=practicalmeteor:mocha --port 4000",
    "sshkey": "eval \"$(ssh-agent -s)\" && ssh-add ~/.ssh/id_rsa",
    "reconfig": "mup reconfig --settings=settings-prod.json",
    "setup": "mup setup --settings=settings-prod.json",
    "proxy-reconfig": "mup proxy reconfig-shared --settings=settings-prod.json",
    "deploy": "mup deploy --settings=settings-prod.json"
  },
  "repository": {
